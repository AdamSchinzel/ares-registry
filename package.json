{
  "name": "ares-registry",
  "version": "1.0.1",
  "description": "Node module for interacting with the ARES Registry",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run format && npm run lint",
    "preversion": "npm run format && npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "check-updates": "npx npm-check-updates",
    "check-updates:minor": "npx npm-check-updates --target minor",
    "check-updates:patch": "npx npm-check-updates --target patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdamSchinzel/ares-registry.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "ARES"
  ],
  "author": "Adam Schinzel",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@AdamSchinzel"
  },
  "bugs": {
    "url": "https://github.com/AdamSchinzel/ares-registry/issues"
  },
  "homepage": "https://github.com/AdamSchinzel/ares-registry#readme",
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "camaro": "^6.2.0"
  }
}
